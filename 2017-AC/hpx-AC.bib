@Article{	  veldhuizen95a,
  abstract	= {{Template metaprograms can generate useful code when
		  interpreted by the compiler, which results in large speed
		  increases for many applications. A simple example that
		  generates factorials at compile time is presented. Although
		  this technique might seem like just a cute C++ trick, it
		  becomes powerful when combined with normal C++ code. In
		  this hybrid approach, source code contains two programs:
		  the normal C++ runtime program, and a template metaprogram
		  that runs at compile time. Template metaprograms can
		  generate useful code when interpreted by the compiler, such
		  as a massively inlined algorithm-that is, an implementation
		  of an algorithm that works for a specific input size, and
		  has its loops unrolled. This results in large speed
		  increases for many applications. The article presents a
		  simple template metaprogram that generates a bubble sort
		  algorithm and its performance is analysed.}},
  author	= {Veldhuizen, Todd},
  citeulike-article-id={4022828},
  citeulike-linkout-0={http://extreme.indiana.edu/\~{}tveldhui/papers/Template-Metaprograms/meta-art.html},
